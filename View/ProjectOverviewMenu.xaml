<Window x:Class="View.ProjectOverviewMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="ProjectOverviewMenu" Height="450" Width="800">
        
    <Grid>
        <ListView ItemsSource="{Binding Projekts}">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Projekt ID" DisplayMemberBinding="{Binding ProjectID}"/>
                    <GridViewColumn Header="Projekt Navn" DisplayMemberBinding="{Binding ProjectName}"/>
                    <GridViewColumn Header="Min Budget" DisplayMemberBinding="{Binding MinBudget}"/>
                    <GridViewColumn Header="Max Budget" DisplayMemberBinding="{Binding MaxBudget}"/>
                    <GridViewColumn Header="Start Dato" DisplayMemberBinding="{Binding StartDate}"/>
                    <GridViewColumn Header="Slut Dato" DisplayMemberBinding="{Binding FinishDate}"/>
                    



                </GridView>
               
            </ListView.View>
        </ListView>
    </Grid>
    
</Window>
